slack_reporter:
    - channel: '#jwsqe-pit'
      job_names:
        - jws-interop-aws
        - jws-interop-aws-fips
        - periodic-ci-jws-qe-interop-ocp-ci-main-ocp-4.14-lp-interop-jws-interop-aws
        - periodic-ci-jws-qe-interop-ocp-ci-main-ocp-4.14-lp-interop-jws-interop-ibmcloud
        - periodic-ci-jws-qe-interop-ocp-ci-main-ocp-4.15-lp-interop-jws-interop-aws
        - periodic-ci-jws-qe-interop-ocp-ci-main-ocp-4.15-lp-interop-jws-interop-ibmcloud
        - periodic-ci-jws-qe-interop-ocp-ci-main-ocp-4.16-lp-interop-jws-interop-aws
        - periodic-ci-jws-qe-interop-ocp-ci-main-ocp-4.16-lp-interop-jws-interop-ibmcloud
        - periodic-ci-jws-qe-interop-ocp-ci-main-ocp-4.17-lp-interop-jws-interop-aws
        - periodic-ci-jws-qe-interop-ocp-ci-main-ocp-4.17-lp-interop-jws-interop-aws-fips
        - periodic-ci-jws-qe-interop-ocp-ci-main-ocp-4.18-lp-interop-jws-interop-aws
        - periodic-ci-jws-qe-interop-ocp-ci-main-ocp-4.18-lp-interop-jws-interop-aws-fips
        - periodic-ci-jws-qe-interop-ocp-ci-main-ocp-4.19-lp-interop-jws-interop-aws
        - periodic-ci-jws-qe-interop-ocp-ci-main-ocp-4.19-lp-interop-jws-interop-aws-fips
      job_states_to_report:
        - success
        - failure
        - error
      report_template: '{{if eq .Status.State "success"}} :slack-green: Job *{{.Spec.Job}}* ended with *{{.Status.State}}*. <{{.Status.URL}}|View logs> {{else}} :failed: Job *{{.Spec.Job}}* ended with *{{.Status.State}}*. <{{.Status.URL}}|View logs> {{end}}'
