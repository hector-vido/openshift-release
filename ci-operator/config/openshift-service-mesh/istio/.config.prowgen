slack_reporter:
    - channel: '#istio-notifications'
      job_names:
        - periodic-ci-openshift-service-mesh-istio-master-sync-upstream-istio-master
        - periodic-ci-openshift-service-mesh-istio-master-sync-upstream-istio-master
      job_states_to_report:
