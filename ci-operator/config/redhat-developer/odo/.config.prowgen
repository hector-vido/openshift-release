slack_reporter:
    - channel: '#odo-notifications'
      job_names:
        - odo-scenario-aws
        - odo-scenario-aws-fips
        - periodic-ci-redhat-developer-odo-main-odo-ocp4.13-lp-interop-odo-scenario-aws
        - periodic-ci-redhat-developer-odo-main-odo-ocp4.14-lp-interop-odo-scenario-ibmcloud
        - periodic-ci-redhat-developer-odo-main-odo-ocp4.15-lp-interop-odo-scenario-ibmcloud
        - periodic-ci-redhat-developer-odo-main-odo-ocp4.16-lp-interop-odo-scenario-ibmcloud
        - periodic-ci-redhat-developer-odo-main-odo-ocp4.17-lp-interop-odo-scenario-aws
        - periodic-ci-redhat-developer-odo-main-odo-ocp4.17-lp-interop-odo-scenario-aws-fips
        - periodic-ci-redhat-developer-odo-main-odo-ocp4.17-lp-interop-odo-scenario-ibmcloud
        - periodic-ci-redhat-developer-odo-main-odo-ocp4.18-lp-interop-odo-scenario-aws
        - periodic-ci-redhat-developer-odo-main-odo-ocp4.18-lp-interop-odo-scenario-aws-fips
        - periodic-ci-redhat-developer-odo-main-odo-ocp4.19-lp-interop-odo-scenario-aws
        - periodic-ci-redhat-developer-odo-main-odo-ocp4.19-lp-interop-odo-scenario-aws-fips
      job_states_to_report:
        - success
        - failure
        - error
      report_template: '{{if eq .Status.State "success"}} :slack-green: Job *{{.Spec.Job}}* ended with *{{.Status.State}}*. <{{.Status.URL}}|View logs> {{else}} :failed: Job *{{.Spec.Job}}* ended with *{{.Status.State}}*. <{{.Status.URL}}|View logs> {{end}}'
