slack_reporter:
    - channel: '#forum-ocp-storage'
      job_names:
        - periodic-e2e-aws-efs-csi
        - periodic-e2e-smb-csi
        - periodic-e2e-azure-file-csi
      job_states_to_report:
        - failure
        - error
      report_template: ':red_jenkins_circle: Job *{{.Spec.Job}}* ended with *{{.Status.State}}*. <{{.Status.URL}}|View logs>'
