slack_reporter:
    - channel: '#java-qe-infrastructure-monitoring'
      job_names:
        - openjdk-interop-aws
        - openjdk-interop-aws-fips
        - periodic-ci-rh-openjdk-jdkContainerOcpTests-main-openjdk-ocp4.14-lp-interop-openjdk-interop-aws
        - periodic-ci-rh-openjdk-jdkContainerOcpTests-main-openjdk-ocp4.14-lp-interop-openjdk-interop-ibmcloud
        - periodic-ci-rh-openjdk-jdkContainerOcpTests-main-openjdk-ocp4.15-lp-interop-openjdk-interop-aws
        - periodic-ci-rh-openjdk-jdkContainerOcpTests-main-openjdk-ocp4.15-lp-interop-openjdk-interop-ibmcloud
        - periodic-ci-rh-openjdk-jdkContainerOcpTests-main-openjdk-ocp4.16-lp-interop-openjdk-interop-aws
        - periodic-ci-rh-openjdk-jdkContainerOcpTests-main-openjdk-ocp4.16-lp-interop-openjdk-interop-ibmcloud
        - periodic-ci-rh-openjdk-jdkContainerOcpTests-main-openjdk-ocp4.17-lp-interop-openjdk-interop-ibmcloud
        - periodic-ci-rh-openjdk-jdkContainerOcpTests-main-openjdk-ocp4.18-lp-interop-openjdk-interop-aws
        - periodic-ci-rh-openjdk-jdkContainerOcpTests-main-openjdk-ocp4.18-lp-interop-openjdk-interop-aws-fips
        - periodic-ci-rh-openjdk-jdkContainerOcpTests-main-openjdk-ocp4.19-lp-interop-openjdk-interop-aws
        - periodic-ci-rh-openjdk-jdkContainerOcpTests-main-openjdk-ocp4.19-lp-interop-openjdk-interop-aws-fips
      job_states_to_report:
        - success
        - failure
        - error
      report_template: '{{if eq .Status.State "success"}} :slack-green: Job *{{.Spec.Job}}* ended with *{{.Status.State}}*. <{{.Status.URL}}|View logs> {{else}} :failed: Job *{{.Spec.Job}}* ended with *{{.Status.State}}*. <{{.Status.URL}}|View logs> {{end}}'
