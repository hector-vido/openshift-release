slack_reporter:
    - channel: '#sandbox-ci-reports'
      job_names:
        - e2e
        - ci-daily
      job_states_to_report:
        - success
        - failure
        - error
      report_template: '{{ if eq .Status.State "success" }} :succeeded: {{ else }} :failed: {{ end }} Job *{{.Spec.Job}}* ended with *{{.Status.State}}*. <{{.Status.URL}}|View logs>'
